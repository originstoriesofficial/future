{
    "compilerOptions": {
      "jsx": "react-jsx",
      "baseUrl": ".",
      "paths": {
        "@/*": ["./*"]
      },
      "moduleResolution": "node",
      "target": "esnext",
      "module": "esnext",
      "lib": ["dom", "esnext"],
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true
    }
  }
  